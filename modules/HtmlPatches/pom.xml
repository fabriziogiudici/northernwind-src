    <groupId>it.tidalwave.northernwind</groupId>
    <artifactId>it-tidalwave-html-patches</artifactId>
    <version>1.2-ALPHA-2-SNAPSHOT</version>
    <packaging>jar</packaging>
    <name>NorthernWind - HTML Patches</name>
    <description>Some patched classes for manipulating HTML.</description>

    <properties>
        <tft.javac.xlint>-Xlint:none</tft.javac.xlint> <!-- It's obsolete code with lots of warnings -->
        <tft.javac.showDeprecation>false</tft.javac.showDeprecation>
        <tft.javac.showWarnings>false</tft.javac.showWarnings>
        <tft.test.skip>true</tft.test.skip> <!-- Patched files, don't count in JaCoCo -->
        <!-- Otherwise can't be released because of missing javadocs -->
        <tft.javadoc.excludePackageNames />
    </properties>


</project>
